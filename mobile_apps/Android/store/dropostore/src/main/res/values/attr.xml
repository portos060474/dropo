<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomTextView">
        <attr name="appTextColor" format="integer" />
        <attr name="appBackgroundColor" format="integer" />
        <attr name="appBackgroundStroke" format="dimension" />
        <attr name="appBackgroundStrokeColor" format="integer" />
        <attr name="appDrawableColor" format="integer" />
        <attr name="typefaceTv" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomFontTextViewTitle">
        <attr name="appTextColorTitle" format="integer" />
    </declare-styleable>
    <declare-styleable name="CustomInputEditText">
        <attr name="typefaceInputEt" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="typefaceEt" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="appTextButtonColor" format="integer" />
        <attr name="typefaceBtn" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomRatingBar">
        <attr name="appRatingBarColor" format="integer" />
    </declare-styleable>
    <declare-styleable name="CustomProgressBar">
        <attr name="appProgressBarColor" format="integer" />
    </declare-styleable>
    <declare-styleable name="CustomFloatingButton">
        <attr name="appFBBackgroundColor" format="integer" />
    </declare-styleable>

    <declare-styleable name="ExpandablePanel">
        <attr name="handle" format="reference" />
        <attr name="content" format="reference" />
        <attr name="animationDuration" format="integer" />
        <attr name="collapsedHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CustomCircularProgressView">
        <attr name="cpv_progress" format="float" />
        <attr name="cpv_maxProgress" format="float" />
        <attr name="cpv_animDuration" format="integer" />
        <attr name="cpv_animSwoopDuration" format="integer" />
        <attr name="cpv_animSyncDuration" format="integer" />
        <attr name="cpv_color" format="color" />
        <attr name="cpv_thickness" format="dimension" />
        <attr name="cpv_indeterminate" format="boolean" />
        <attr name="cpv_animAutostart" format="boolean" />
        <attr name="cpv_animSteps" format="integer" />
        <attr name="cpv_startAngle" format="float" />
    </declare-styleable>

    <declare-styleable name="CustomView">
        <attr name="drawableLeftCompat" format="reference" />
        <attr name="drawableRightCompat" format="reference" />
        <attr name="drawableTopCompat" format="reference" />
        <attr name="drawableBottomCompat" format="reference" />
    </declare-styleable>

    <declare-styleable name="MultiLineLayout">
        <attr name="horizontalSpacing" format="dimension" />
        <attr name="verticalSpacing" format="dimension" />
    </declare-styleable>
    <declare-styleable name="FlowLayout_LayoutParams">
        <attr name="layout_horizontalSpacing" format="dimension" />
        <attr name="layout_breakLine" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PinViewTheme">
        <attr name="pinViewStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="PinView">
        <attr name="itemCount" format="integer" />
        <attr name="itemWidth" format="dimension" />
        <attr name="itemHeight" format="dimension" />
        <attr name="itemRadius" format="dimension" />
        <attr name="itemSpacing" format="dimension" />
        <attr name="lineWidth" format="dimension" />
        <attr name="lineColor" format="reference|color" />
        <attr name="viewType" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="line" value="1" />
        </attr>
        <attr name="android:cursorVisible" />
        <attr name="cursorWidth" format="dimension" />
        <attr name="cursorColor" format="reference|color" />
    </declare-styleable>

    <!--    theme resources-->
    <attr name="appThemeScreenBackgroundStyle" format="reference" />
    <attr name="appThemeTextViewStyle" format="reference" />
    <attr name="appThemeButtonStyle" format="reference" />
    <attr name="appThemeTextInputLayoutStyle" format="reference" />
    <attr name="appThemeLabelTextStyle" format="reference" />
    <attr name="appThemeBottomLineDividerStyle" format="reference" />
    <attr name="appThemeEditTextStyle" format="reference" />
    <attr name="appThemeCheckBoxStyle" format="reference" />
    <attr name="appThemeRadioButtonStyle" format="reference" />
    <attr name="appThemeSwitchStyle" format="reference" />
    <attr name="appThemeIcon" format="reference" />
    <attr name="appThemeDialogBackgroundStyle" format="reference" />
    <attr name="appThemeModeColor" format="reference" />
    <attr name="appThemeTabStyle" format="reference" />
    <attr name="appThemeTagStyle" format="reference" />
    <attr name="appThemeDividerColor" format="reference" />
    <integer name="appThemeColor">6</integer>

    <declare-styleable name="TagLayout">
        <attr name="tag_layout_mode" format="enum">
            <enum name="normal" value="201" />
            <enum name="edit" value="202" />
            <enum name="change" value="203" />
            <enum name="single_choice" value="204" />
            <enum name="multi_choice" value="205" />
        </attr>
        <attr name="tag_layout_shape" format="enum">
            <enum name="round_rect" value="101" />
            <enum name="arc" value="102" />
            <enum name="rect" value="103" />
        </attr>
        <attr name="tag_layout_random_color" format="boolean" />
        <attr name="tag_layout_press_feedback" format="boolean" />
        <!-- TagLayout -->
        <attr name="tag_layout_bg_color" format="reference|color" />
        <attr name="tag_layout_border_color" format="color" />
        <attr name="tag_layout_border_width" format="dimension" />
        <attr name="tag_layout_border_radius" format="dimension" />
        <attr name="tag_layout_vertical_interval" format="dimension" />
        <attr name="tag_layout_horizontal_interval" format="dimension" />
        <attr name="tag_layout_fit_num" format="integer" />
        <attr name="tag_layout_horizontal_reverse" format="boolean" />
        <!-- TagView -->
        <attr name="tag_view_bg_color" format="color" />
        <attr name="tag_view_border_color" format="color" />
        <attr name="tag_view_text_color" format="color" />
        <attr name="tag_view_bg_color_check" format="color" />
        <attr name="tag_view_border_color_check" format="color" />
        <attr name="tag_view_text_color_check" format="color" />
        <attr name="tag_view_border_width" format="dimension" />
        <attr name="tag_view_border_radius" format="dimension" />
        <attr name="tag_view_text_size" format="dimension" />
        <attr name="tag_view_vertical_padding" format="dimension" />
        <attr name="tag_view_horizontal_padding" format="dimension" />
        <attr name="tag_view_icon_padding" format="dimension" />
    </declare-styleable>

    <!-- TagView -->
    <declare-styleable name="TagView">
        <attr name="tag_mode" format="enum">
            <enum name="normal" value="201" />
            <enum name="check" value="204" />
            <enum name="icon_check_invisible" value="206" />
            <enum name="icon_check_change" value="207" />
        </attr>
        <attr name="tag_shape" format="enum">
            <enum name="round_rect" value="101" />
            <enum name="arc" value="102" />
            <enum name="rect" value="103" />
        </attr>
        <!-- 3、5 是Gravity里定义的值 -->
        <attr name="tag_gravity" format="enum">
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="tag_press_feedback" format="boolean" />
        <attr name="tag_auto_check" format="boolean" />
        <attr name="tag_checked" format="boolean" />
        <attr name="tag_bg_color" format="color" />
        <attr name="tag_border_color" format="color" />
        <attr name="tag_text_color" format="color" />
        <attr name="tag_bg_color_check" format="color" />
        <attr name="tag_border_color_check" format="color" />
        <attr name="tag_text_color_check" format="color" />
        <attr name="tag_text_check" format="string" />
        <attr name="tag_border_width" format="dimension" />
        <attr name="tag_border_radius" format="dimension" />
        <attr name="tag_vertical_padding" format="dimension" />
        <attr name="tag_horizontal_padding" format="dimension" />
        <attr name="tag_icon_padding" format="dimension" />
        <attr name="tag_icon" format="reference" />
        <attr name="tag_icon_change" format="reference" />
        <attr name="tag_text" format="string" />
        <attr name="tag_text_size" format="dimension" />
        <attr name="lineMargin" format="dimension" />
        <attr name="tagMargin" format="dimension" />
        <attr name="textPaddingLeft" format="dimension" />
        <attr name="textPaddingRight" format="dimension" />
        <attr name="textPaddingTop" format="dimension" />
        <attr name="textPaddingBottom" format="dimension" />
    </declare-styleable>
</resources>